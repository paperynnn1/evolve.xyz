--[evolve.xyz source]--

--- Tables Section ---
local rage = {
    movement = {
        enabled = false,
        speed = 0
    },
    freezeworld = {
        enabled = false
    }
}
local misc = {
    exploits = {
        antifling = false,
        antistomp = false,
        antibag = false
    }
}
local visuals = {
    esp = {
        box = false,
        healthbar = false,
        tracer = false,
        names = false,
        skeleton = false,
        colours = Color3.fromRGB(0,85,255)
    },
    spoof = {
        ping = false,
        memory = false
    }
}

--- Loadstring Section ---
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/cat"))()
local Loader = loadstring(game:HttpGet('https://raw.githubusercontent.com/paperynnn1/hornyware/main/.gg/loader'))()
wait(7.99)
local Window = Library:CreateWindow("evolve", Vector2.new(495, 595), Enum.KeyCode.RightShift)

--- Locals Section ---
local RageTab = Window:CreateTab("Rage")
local VisualsTab = Window:CreateTab("Visuals")
local SettingsTab = Window:CreateTab("Settings")

local MSection = RageTab:CreateSector("Movement", "left")
local ESection = RageTab:CreateSector("Exploits", "left")
local FSection = RageTab:CreateSector("Freeze World", "right")

local CSection = VisualsTab:CreateSector("CrossHair", "right")
local ESPSection = VisualsTab:CreateSector("Esp", "left")
local SPSection = VisualsTab:CreateSector("Spoofers", "left")

--- Rage Section ---
---CFRAME
MSection:AddToggle('Enabled', false, function(e)
    rage.movement.enabled = e
end)
MSection:AddSlider("Speed Value", 0, 0, 10, 0.5, function(e)
    rage.movement.speed = e
end)

---FREZEWORLD
FSection:AddToggle('Enabled', false, function(e)
    rage.freezeworld.enabled = e
end)

---FLOATING
ESection:AddToggle('Anti Fling', false, function(e)
    misc.exploits.antifling = e
end)
ESection:AddToggle('Anti Stomp', false, function(e)
   misc.exploits.antistomp = e
end)
ESection:AddToggle('Anti Bag', false, function(e)
   misc.exploits.antibag = e
end)

--- Visuals Section ---
---ESP
ESPSection:AddToggle('Box', false, function(e)
    visuals.esp.box = e
end)
ESPSection:AddToggle('Health', false, function(e)
    visuals.esp.healthbar = e
end)
ESPSection:AddToggle('Tracers', false, function(e)
    visuals.esp.tracer = e
end)
ESPSection:AddToggle('Skeleton', false, function(e)
    visuals.esp.skeleton = e
end)
ESPSection:AddToggle('Names', false, function(e)
    visuals.esp.names = e
end)
ESPSection:AddColorpicker('Esp Colours',Color3.fromRGB(0,85,255), function(e)
    visuals.esp.colours = e
end)

---CROSSHAIR
CSection:AddToggle('Top', true, function(e)
    game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.Aim.Top.Visible = e
end)
CSection:AddToggle('Bottom', true, function(e)
    game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.Aim.Bottom.Visible = e
end)
CSection:AddToggle('Right', true, function(e)
    game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.Aim.Right.Visible = e
end)
CSection:AddToggle('Left', true, function(e)
    game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.Aim.Left.Visible = e
end)

---SPOOF
SPSection:AddToggle('Ping', false, function(e)
    visuals.spoof.ping = e
end)
SPSection:AddToggle('Memory', false, function(e)
    visuals.spoof.memory = e
end)

---CONFIG
SettingsTab:CreateConfigSystem("left")

--- Table Functions ---
game:GetService("RunService").Heartbeat:Connect(function()   
    if rage.movement.enabled then
        for _, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v:IsA("Script") and v.Name ~= "Health" and v.Name ~= "Sound" and v:FindFirstChild("LocalScript") then
                v:Destroy()
            end
        end
        game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
            repeat
                wait()
            until game.Players.LocalPlayer.Character
            char.ChildAdded:Connect(function(child)
                if child:IsA("Script") then 
                    wait(0.1)
                    if child:FindFirstChild("LocalScript") then
                        child.LocalScript:FireServer()
                    end
                end
            end)
        end)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame +
        game.Players.LocalPlayer.Character.Humanoid.MoveDirection * rage.movement.speed
    end 
    if misc.exploits.antifling == true then
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    elseif misc.exploits.antifling == false then
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    end
    if misc.exploits.antistomp then
        if game.Players.LocalPlayer.Character.Humanoid.Health <= 5 then
            for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA("BasePart") then
                    v:Destroy()
                end
            end
        end
    end
    if misc.exploits.antibag then
        game.Players.LocalPlayer.Character["Christmas_Sock"]:Destroy()
    end
    if rage.freezeworld.enabled == true then
        settings().Network.IncomingReplicationLag = 999999999
    elseif rage.freezeworld.enabled == false then
        settings().Network.IncomingReplicationLag = 0
    end
    if visuals.spoof.ping then
        local PerformanceStats = game:GetService("CoreGui"):WaitForChild("RobloxGui"):WaitForChild("PerformanceStats");
        local PingLabel;
        for I, Child in next, PerformanceStats:GetChildren() do
            if Child.StatsMiniTextPanelClass.TitleLabel.Text == "Ping" then
                PingLabel = Child.StatsMiniTextPanelClass.ValueLabel;
                break;
            end;
        end;
        
        local text = "30.56 ms";
        PingLabel:GetPropertyChangedSignal("Text"):Connect(function()
            PingLabel.Text = text;
        end);
        PingLabel.Text = text;
    end
    if visuals.spoof.memory then
        local PerformanceStats = game:GetService("CoreGui"):WaitForChild("RobloxGui"):WaitForChild("PerformanceStats");
        local MemLabel;
        for I, Child in next, PerformanceStats:GetChildren() do
            if Child.StatsMiniTextPanelClass.TitleLabel.Text == "Mem" then
                MemLabel = Child.StatsMiniTextPanelClass.ValueLabel;
                break;
            end;
        end;
        
        local text = "562.77 MB" ;
        MemLabel:GetPropertyChangedSignal("Text"):Connect(function()
            MemLabel.Text = text;
        end);
        MemLabel.Text = text;
    end
end)
